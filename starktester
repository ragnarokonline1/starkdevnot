#[contract]
mod GooddayStarknet {
    use starknet::get_caller_address;
    use starknet::ContractAddress;

#module

    #[external]
    fn Byebye(from: ContractAddress, value: felt252) {}


    #[internal]
    fn Say_Byemessage: felt252) {
        let caller = get_caller_address();
        Hello(caller, message);
    }

}

